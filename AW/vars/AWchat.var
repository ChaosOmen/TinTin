#CLASS {chat} OPEN

#ALIAS {chatdown}
{
     #if {$chat[page] > 1}
     {
          #math chat[page] {$chat[page]-1};
          show_chat
     }
}

#ALIAS {chatend}
{
     #math chat[page] 1;
     show_chat
}

#ALIAS {chatup}
{
     #if {$chat[rows]*{$chat[page]+1} <= $chat[max] && $chat[rows]*{$chat[page]+1} <= &chat[log][]}
     {
          #math chat[page] {$chat[page]+1};
          show_chat
     }
}

#ALIAS {show_chat}
{
     #math chat[start]	{$chat[rows] * $chat[page]};
     #math chat[end]		{$chat[rows] * $chat[page] - $chat[rows] + 1};
     #line sub {var;esc} #draw tile $chat[row][start] $chat[col][start] $chat[row][end] $chat[col][end] $chat[log][-$chat[start]..-$chat[end]]
}

#ALIAS {update_chat}
{
     #if {&chat[log][] > $chat[max]}
     {
          #list chat[log] del 1
     };
     #list chat[log] ins -1 {%0};
     #class {chat} {write} {.tt/AW/vars/${mud}chat.var};
     show_chat
}

