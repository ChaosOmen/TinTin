#class {common} {open}

/* TMUX Aliases */
#alias {rs} {
	#sys tmux resize-pane -t 1 -x 45 -y 17;
	#map get {roomvnum} {roomnum};
	#map goto $roomnum
}

#alias {rsaw} {
	#sys tmux resize-pane -t 1 -x 30 -y 20;
}


/* TT Aliases */
#alias {ascii} {
	converse %1;
	#textin .tt/ascii/%2;
	~q
}

#alias {/calc} {
	#math {math} {%1};
	#echo $math
}

#class {common} {close}
